{
  "name": "mygao-test",
  "version": "0.0.9",
  "description": "一套代码兼容多个版本",
  "author": "mygao666",
  "license": "ISC",
  "keywords": [
    "vue-demi",
    "vue2",
    "vue3"
  ],
  "repository": {
    "type": "git",
    "url": "https:/https://github.com/io-o/mygao-test.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build"
  },
  "main": "./dist/mygao-test.umd.js",
