  },
  "peerDependencies": {
    "vue": "^2.6.11 || >=3.2.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^16.9.2",
    "@vitejs/plugin-vue": "^1.4.0",
    "@vue/compiler-sfc": "3.2.12",
    "cross-env": "^7.0.3",
    "less": "^4.1.1",
    "typescript": "^4.4.3",
    "vite": "^2.5.8",
    "vite-plugin-dts": "^0.5.3",
    "vue": "3.2.12",
    "vue-tsc": "0.3.0"
  }
